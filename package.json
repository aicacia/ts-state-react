{
  "name": "@aicacia/state-react",
  "version": "0.1.3",
  "description": "connect react components with @aicacia/state stores",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/aicacia/libs/ts-state-react.git"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "doc": "typedoc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node node_modules/.bin/tape src/**/*.test.{ts,tsx} | tap-spec",
    "coverage": "nyc --reporter=lcov --require ts-node/register tape src/**/*.test.{ts,tsx} | tap-spec; nyc report ---reporter=text",
    "clean": "rm -rf ./lib",
    "format": "prettier-tslint fix '**/*.{ts,tsx}' && prettier --write '**/*.{js,jsx,json}' --ignore-path .gitignore",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -e 'src/**/*.test.*'",
    "todolist-connect": "parcel examples/todolist-connect/index.html",
    "todolist-hooks": "parcel examples/todolist-hooks/index.html"
  },
  "keywords": [
    "state",
    "react"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jsdom": "^16.2.3",
    "@types/react": "^16.9.43",
    "@types/tape": "^4.13.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jsdom": "^16.3.0",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "prettier-tslint": "^0.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tap-nyc": "^1.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": ">= 16.8.6"
  },
  "dependencies": {
    "@aicacia/state": "^0.1.2",
    "shallow-equal-object": "^1.1.1"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.test.*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  }
}
