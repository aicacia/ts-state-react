{
  "name": "@aicacia/state-react",
  "version": "0.2.0",
  "description": "connect react components with @aicacia/state stores",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/aicacia/libs/ts-state-react.git"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "doc": "typedoc --entryPoints src",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node node_modules/.bin/tape src/**/*.test.{ts,tsx} | tap-spec",
    "coverage": "nyc --reporter=lcov --check-coverage --lines=75 --require ts-node/register tape src/**/*.test.{ts,tsx} | tap-spec; nyc report ---reporter=text",
    "clean": "rm -rf ./lib",
    "lint": "eslint . --fix",
    "todolist-connect": "parcel examples/todolist-connect/index.html",
    "todolist-hooks": "parcel examples/todolist-hooks/index.html"
  },
  "keywords": [
    "state",
    "react"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@aicacia/state": "^0.2.3",
    "@testing-library/react": "^11.2.2",
    "@types/jsdom": "^16.2.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "immutable": "^4.0.0-rc.12",
    "jsdom": "^16.4.0",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tap-nyc": "^1.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typedoc": "beta",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "tslib": ">=2",
    "@aicacia/state": ">=0.2",
    "react": ">=16"
  },
  "dependencies": {
    "shallow-equal-object": "^1.1.1"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.test.*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  }
}
